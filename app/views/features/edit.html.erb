<main class="container">
  <%= link_to sanitize("&larr; Back to property"), property_path(@property) %>

  <div class="header">
    <h1>Edit feature</h1>
  </div>

  <%= turbo_frame_tag @feature do %>
    <%= render "form",
            property: @property,
            space: @space,
            feature: @feature %>
  <% end %>
</main>
