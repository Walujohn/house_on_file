<main class="container">
  <%= link_to sanitize("&larr; Back to property"), property_path(@property) %>

  <div class="header">
    <h1>New space</h1>
    <%= link_to "New space",
                new_property_space_path(@property),
                data: { turbo_frame: dom_id(Space.new) },
                class: "btn btn--primary" %>
  </div>
    
  <%= turbo_frame_tag @space do %>
    <%= render "form", property: @property, space: @space %>
  <% end %>
</main>
