<main class="container">
  <%= link_to sanitize("&larr; Back to property"), property_path(@property) %>

  <div class="header">
    <h1>New space</h1>
  </div>
    
  <%= turbo_frame_tag @space do %>
    <%= render "form", property: @property, space: @space %>
    <div class="empty-state">
      <%= render "list_of_forms", property: @property, space: @space %>
    </div>
  <% end %>
</main>